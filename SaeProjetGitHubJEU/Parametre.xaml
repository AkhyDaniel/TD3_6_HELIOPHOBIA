<Window x:Class="SaeProjetGitHubJEU.Parametre"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SaeProjetGitHubJEU"
        mc:Ignorable="d"
        Title="Parametre" Height="450" Width="800">
    <Grid RenderTransformOrigin="0.503,0.588">
        <Label x:Name="labParametre" Content="" HorizontalAlignment="Center" Margin="0,56,0,0" VerticalAlignment="Top" Height="82" Width="440">
            <Label.Background>
                <ImageBrush ImageSource="/imageButParaF.png"/>
            </Label.Background>
        </Label>
        <Slider HorizontalAlignment="Center" VerticalAlignment="Top" Width="216" Height="23" ValueChanged="Slider_ValueChanged" Margin="0,242,0,0"/>
        <Button x:Name="butOk"  HorizontalAlignment="Left" Margin="140,344,0,0" VerticalAlignment="Top" Height="43" Width="182">
            <Button.Background>
                <ImageBrush ImageSource="/imagesBut/butOK.png"/>
            </Button.Background>
        </Button>
        <Button x:Name="butAnnuler"  HorizontalAlignment="Left" Margin="485,0,0,47" VerticalAlignment="Bottom" Width="196" Height="43" Click="butAnnuler_Click">
            <Button.Background>
                <ImageBrush ImageSource="/imagesBut/butAnnuler.png"/>
            </Button.Background>
        </Button>
        <Label x:Name="labVolume" Content="Volume" HorizontalAlignment="Center" Margin="0,191,0,0" VerticalAlignment="Top" Height="26" Width="60"/>

    </Grid>
</Window>
