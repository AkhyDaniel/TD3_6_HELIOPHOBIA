<Window x:Class="SaeProjetGitHubJEU.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SaeProjetGitHubJEU"
        mc:Ignorable="d"
        Title="Heliophobia" Height="900" Width="1400" ResizeMode="NoResize"  WindowStartupLocation="CenterScreen">
    <Window.Background>
        <ImageBrush ImageSource="/LOGOF.png"/>
    </Window.Background>
    <Grid>
        <ContentControl x:Name="ZoneLobby" Margin="-10,-10,10,10">
            <Menu Height="20" 
      HorizontalAlignment="Left"
      VerticalAlignment="Top"
      Margin="10,10,0,0">
                <MenuItem x:Name="parametremenu" Header="_Parametre" 
             
              Width="68" 
              Background="White" Click="parametremenu_Click"/>
                <MenuItem x:Name="cheatmenu" Header="_Cheats" 
              Height="22" 
              Width="58" Click="cheatmenu_Click"/>
            </Menu>



        </ContentControl>
    </Grid>





</Window>
